{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.69",
        "symfony/asset": "*",
        "symfony/browser-kit": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/maker-bundle": "^1.7",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/test-pack": "^1.0",
        "symfony/twig-bundle": "*",
        "symfony/web-server-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.69",
        "symfony/asset": "*",
        "symfony/browser-kit": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/maker-bundle": "^1.7",
        "symfony/orm-pack": "^1.0",
        "symfony/phpunit-bridge": "4.1.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "*",
        "symfony/web-server-bundle": "*",
        "symfony/yaml": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    },
    "repositories": {
        "phpunit-bridge": {
            "type": "path",
            "url": "C:\\Users\\khoob\\Desktop\\StoryCompanion\\vendor\\symfony\\phpunit-bridge"
        }
    }
}
